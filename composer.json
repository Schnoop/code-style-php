{
    "name": "antwerpes/code-style-php",
    "description": "ESC configuration for PHP projects",
    "keywords": [
        "antwerpes",
        "code-style-php"
    ],
    "homepage": "https://github.com/antwerpes/code-style-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "elisha.witte@antwerpes.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "nunomaduro/larastan": "^2.0",
        "phpro/grumphp": "^1.8",
        "slevomat/coding-standard": "^7.0",
        "spatie/laravel-package-tools": "^1.11",
        "symplify/coding-standard": "^10.0",
        "symplify/easy-coding-standard": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Antwerpes\\CodeStyle\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antwerpes\\CodeStyle\\CodeStyleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
